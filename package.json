{
  "name": "@basster/react-symfony-debug-toolbar",
  "description": "Symfony Web Debug Toolbar flavor for React",
  "author": "Ole Rößner",
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Basster/react-symfony-debug-toolbar"
  },
  "files": ["dist"],
  "main": "./dist/react-symfony-debug-toolbar.umd.js",
  "module": "./dist/react-symfony-debug-toolbar.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-symfony-debug-toolbar.es.js",
      "require": "./dist/react-symfony-debug-toolbar.umd.js"
    }
  },
  "typings": "./src/lib/@types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mswjs/interceptors": "^0.19.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.2.0",
    "prettier": "2.7.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "keywords": [
    "symfony",
    "request",
    "intercept",
    "http",
    "https",
    "debug",
    "react"
  ]
